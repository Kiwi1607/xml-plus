{
  "name": "xml-plus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "dev": "vue-cli-service serve",
    "deploy": "vue-cli-service build && cd extension && bestzip ../deploy/xml-plus.zip *",
    "dev_ext": "vue-cli-service build --mode development --watch",
    "xml_server": "hs -c-1 tests",
    "ext": "run-p dev_ext xml_server",
    "dev_web": "vue-cli-service build --mode development --watch",
    "debug_server": "node server/index.js",
    "server": "node server/index.js",
    "web": "run-p dev_web debug_server",
    "website": "vue-cli-service build",
    "zip": "cd extension && bestzip ../deploy/xml-plus.zip *"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "query-string": "^6.2.0",
    "vue": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "bestzip": "^2.1.2",
    "compression-webpack-plugin": "^2.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "http-server": "^0.11.1",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "run-p": "^0.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-svg-loader": "^0.11.0",
    "vue-template-compiler": "^2.5.17"
  }
}
