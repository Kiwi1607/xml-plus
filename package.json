{
  "name": "xml-plus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist public",
    "build": "vue-cli-service build",
    "dev": "vue-cli-service serve",
    "deploy": "vue-cli-service build && cd dist && bestzip ../deploy/xml-plus.zip *",
    "xml": "hs -c-1 --cors tests -p 8000",
    "watch": "vue-cli-service build --mode development --watch",
    "web-dev": "vue-cli-service serve",
    "web": "run-p web-dev xml",
    "website": "vue-cli-service build"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "vue": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "bestzip": "^2.1.2",
    "compression-webpack-plugin": "^2.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "http-server": "^0.11.1",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-svg-loader": "^0.11.0",
    "vue-template-compiler": "^2.5.17"
  }
}
